{
  "name": "zx",
  "version": "3.1.0",
  "lastVersion": "14",
  "description": "",
  "main": "index.mts",
  "type": "commonjs",
  "environment": "refactor",
  "scripts": {
    "build:all": "concurrently \"pnpm app\"  \"pnpm console\"  \"pnpm wechat\"  \"pnpm qrcode\" \"pnpm park\" ",
    "lingyu an": "pnpm app  & pnpm console & pnpm wechat & pnpm qrcode & pnpm park",
    "prebuild": "ts-node-esm pull.mts -v",
    "build": "concurrently \"pnpm pc\"",
    "postbuild": "ts-node-esm ./src/push.mts",
    "dev:upload": "node index.js -e dev -t upload",
    "dev:preview": "node index.js -e dev -t preview",
    "sit:upload": "node ./src/mini-program/index.js -e sit -t upload",  
    "sit:preview": "node ./src/mini-program/index.js -e sit -t preview",
    "xe:upload": "node ./src/mini-program/index.js -e xe -t upload",
    "xe:preview": "node ./src/mini-program/index.js -e xe -t preview",
    "mini-pull": "ts-node-esm ./src/index.mts -p mini -t -c pull",
    "mini-build": "cd ../mini-deploy && pnpm all",
    "mini-build-ly": "cd ../mini-deploy && pnpm ly",
    "mini-build-xe": "cd ../mini-deploy && pnpm xe",
    "mini-build-sit": "cd ../mini-deploy && pnpm sit",
    "mini-build-dev": "cd ../mini-deploy && pnpm dev",
    "mini-tag": "ts-node-esm ./src/index.mts -p mini -t -c tag",
    "mini": "pnpm mini-pull && pnpm mini-build && pnpm mini-tag",
    "mini-ly": "pnpm mini-pull && pnpm mini-build-ly && pnpm mini-tag",
    "mini-xe": "pnpm mini-pull && pnpm mini-build-xe && pnpm mini-tag",
    "mini-sit": "pnpm mini-pull && pnpm mini-build-sit && pnpm mini-tag",
    "mini-dev": "pnpm mini-pull && pnpm mini-build-dev && pnpm mini-tag",
    "all": "pnpm all-web && pnpm mini",
    "all-web": "pnpm console && pnpm mp",
    "console": "ts-node-esm ./src/index.mts -p console -t -g",
    "console-dev": "ts-node-esm ./src/index.mts -p console",
    "mp": "ts-node-esm ./src/index.mts -p mp -t",
    "app": "ts-node-esm ./src/index.mts -p app",
    "app-ffp": "ts-node-esm ./src/index.mts  -p app -c ffp-app",
    "wechat": "ts-node-esm ./src/index.mts  -p wechat",
    "qrcode": "ts-node-esm ./src/index.mts  -p qrcode",
    "park": "ts-node-esm ./src/index.mts -p park",
    "ffp-mp": "pnpm release-patch && ts-node-esm ./src/index.mts  -p mp -c ffp-mp",
    "robot": "pnpm release-patch && ts-node-esm ./src/index.mts  -p robot -b pnpm",
    "server-d": "ts-node-esm ./src/dotnetcore/build-server.mts -p dvs-dynamic",
    "server-dm": "ts-node-esm ./src/dotnetcore/build-server.mts -p dvs-datamonitor",
    "server-cf": "ts-node-esm ./src/dotnetcore/build-server.mts -p dvs-customform",
    "server-r": "ts-node-esm ./src/dotnetcore/build-server.mts -p dvs-robot",
    "server-q": "ts-node-esm ./src/dotnetcore/build-server.mts -p dvs-query -f 3.1",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch",
    "release": "standard-version",
    "release-version": "standard-version release-as 0.0.2.001",
    "node": "node  --experimental-json-modules ./other/minimist.mjs -a aa -b bb -c cc",
    "fileSave": "node  ./other/fileSave.mjs",
    "sql:sit": "ts-node-esm ./src/sql/index.mts -e sit",
    "sql:ly": "ts-node-esm ./src/sql/index.mts -e ly",
    "sql:xe": "ts-node-esm ./src/sql/index.mts -e xe",
    "gpt": "ts-node-esm ./src/chatgpt.mts",
    "mini-console": "pnpm cd && pnpm console",
    "go": "ts-node-esm ./src/go/build-go.mts",
    "docker": "ts-node-esm ./src/docker-course/index.mts"
  },
  "standard-version": {
    "skip": { 
      "changelog": true,
      "commit": true,
      "tag": true,
      "bump": false
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "zx": "7.1.1"
  },
  "dependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/node": "*",
    "axios": "^1.3.4",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.3.1",
    "enquirer": "^2.3.6",
    "execa": "^6.1.0",
    "file-save": "^0.2.0",
    "file-saver": "^2.0.5",
    "fs": "0.0.1-security",
    "fs-extra": "^10.1.0",
    "gradient-string": "^2.0.1",
    "hjson": "^3.2.2",
    "minimist": "^1.2.6",
    "miniprogram-ci": "^1.9.8",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.1",
    "nanospinner": "^1.1.0",
    "openai": "^3.2.1",
    "semver": "^7.3.7",
    "shortid": "^2.2.16",
    "socks-proxy-agent": "^8.0.1",
    "standard-version": "^9.3.2",
    "typechat": "^0.0.10"
  }
}
